{
  "presets": [
    [
      "@babel/preset-env",
      {
        "loose": true
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "inline-dotenv",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread",
    [
      "import-inspector",
      {
        "serverSideRequirePath": true
      }
    ],
    [
      "module-resolver",
      {
        "root": [
          "./src/"
        ]
      }
    ]
  ]
}
